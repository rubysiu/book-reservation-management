<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xx.dao.AdminDao">

    <select id="findAdmin" resultType="admin">
        SELECT
            id,
            token
        FROM
            admin
        WHERE
            token=#{token}
    </select>

</mapper>
